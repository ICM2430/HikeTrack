<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Contorno verde oscuro -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#0F3313" /> <!-- Verde oscuro -->
        </shape>
    </item>
    <!-- Fondo interior -->
    <item android:insetLeft="4dp" android:insetTop="4dp" android:insetRight="4dp" android:insetBottom="4dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>

